@{
    ViewBag.Title = "Test";
}

@model IList<Launch>

<div class="text-center">

    <div class="launch_container">
        @Model.
        @foreach (var test in ViewBag.Launches)
        {
            <div class="launch">
                <div class="launch_img">
                    <img src="@test.image">
                </div>
                <div class="launch_info">
                    <div class="launch_id">
                        <a asp-controller="RocketLaunches" asp-action="LaunchDetails" asp-route-launchID="@test.id">@test.name</a>
                    </div>
                    <div class="launch_start">
                        @test.window_start
                    </div>
                    <div class="launch_status">
                        <h8>@test.status.description</h8>
                    </div>
                    <div class="favourite_remove">
                        <a asp-controller="RocketLaunches" asp-action="RemoveFromFavourite" asp-route-launchId="@test.id"><span>Remove</span> from favourites</a>
                    </div>
                </div>
            </div>
        }
    </div>
</div>
